<%= form_with(model: model, local: true) do |f| %>
  <%= render 'layouts/error_messages', model: f.object %>
  <div class="form-group">
    <%= f.label :image, '画像をアップロード' %>
    <%= f.file_field :image %>
  </div>
  <div class="form-group">
    <%= f.label :name, '名称' %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <%= f.submit 'アップロードする'%>
<% end %>